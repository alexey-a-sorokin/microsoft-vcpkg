{
  "$comment": "Use the suitesparse/update_suitesparse.py script to update all SuiteSparse ports",
  "name": "suitesparse-umfpack",
  "version-semver": "6.3.4",
  "description": "UMFPACK: Routines solving sparse linear systems via LU factorization in SuiteSparse",
  "homepage": "https://people.engr.tamu.edu/davis/suitesparse.html",
  "license": "GPL-2.0-or-later",
  "dependencies": [
    "suitesparse-amd",
    "suitesparse-config",
    {
      "name": "vcpkg-cmake",
      "host": true
    },
    {
      "name": "vcpkg-cmake-config",
      "host": true
    }
  ],
  "default-features": [
    "cholmod"
  ],
  "features": {
    "cholmod": {
      "description": "Use CHOLMOD",
      "dependencies": [
        "suitesparse-cholmod"
      ]
    }
  }
}
